
<body>
    <div id="wrapper">
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">커뮤니티</h1>
                </div>
            </div>
            
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            목록
                        </div>
                                    
                        <div class="panel-body">
                            <form id="frm" action="" method="get">
    
                            <div id="dataTables-example_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                                <div class="row">
                                    <div class="col-sm-12">
                                        <table class="table table-striped table-bordered table-hover dataTable no-footer dtr-inline"  role="grid" aria-describedby="dataTables-example_info" >
                                            <colgroup>
                                                <col style="width: 5%;"/>
                                                <col style="width: 9%"/>
                                                <col style="width: 25%"/>
                                                <col style="width: 10%"/>
                                                <col style="width: 9%;"/>
                                                <col style="width: 5%;"/>
                                            </colgroup>
                                            <thead>
                                                <tr role="row">
                                                    <th>번호</th>
                                                    <th>나라</th>
                                                    <th>제목</th>
                                                    <th>작성자</th>
                                                    <th>작성일</th>
                                                    <th>조회수</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% boards.forEach(board =>{ %>
                                                    <tr role="row">
                                                        <td><%=board.bbsId%></td>
                                                        <td><%=board.bbsGroup%></td>
                                                        <td class="title">
                                                            <a class="text-ellipsis" href="/boards/<%=board.bbsGroup%>/<%=board.bbsId%>"><%=board.bbsTitle%></a>
                                                        </td>
                                                        <td><%=board.bbsId%></td>
                                                        <td><%=moment(board.bbsRegDate).format('YYYY-MM-DD HH:MM:DD')%></td>
                                                        <td><%=board.bbsViewCnt%></td>
                                                    </tr>
                                               <%});%>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <a href="/boards/<%=params.bbsGroup%>/new"  class="btn btn-default" >등록</a>
                                    </div>
                                </div>
                            </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>